pipeline {
	agent any
	tools {
	  maven 'Maven 3.5'
	  git 'Default'
	  jdk 'Java 10'
	}
	stages {
	    stage("just Trying") {
	    	steps {
	        	echo 'Hello World'
	        }   
	    }
	    stage("Maven Build") {
	    	steps {
	    		bat "mvn clean install"
	    	}
	    }
    }
}